{
  "name": "boathouse-etl",
  "version": "1.3.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "dev": "ts-node src/index.ts",
    "start": "node dist/index.js",
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "validate": "npm run lint && npm run test && npm run build",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "format:check": "prettier --check src/**/*.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "setup": "npx sequelize-cli db:migrate --to 20250115000004-setup-complete-database.js",
    "setup:rollback": "npx sequelize-cli db:migrate:undo --to 20250115000004-setup-complete-database.js",
    "etl": "ts-node src/index.ts",
    "etl:full": "ts-node src/index.ts full",
    "etl:boats": "ts-node src/index.ts boats",
    "etl:usra-categories": "ts-node src/index.ts usra-categories",
    "etl:athletes": "ts-node src/index.ts athletes",
    "etl:teams": "ts-node src/index.ts teams",
    "etl:practice-sessions": "ts-node src/index.ts practice-sessions",
    "etl:attendance": "ts-node src/index.ts attendance",
    "etl:lineup": "ts-node src/index.ts lineup",
    "etl:test": "ts-node src/index.ts test",
    "etl:validate": "ts-node src/index.ts validate",
    "etl:status": "ts-node src/index.ts status",
    "validate:config": "powershell -ExecutionPolicy Bypass -File scripts/validate-config.ps1",
    "migrate:up": "npx sequelize-cli db:migrate",
    "migrate:down": "npx sequelize-cli db:migrate:undo",
    "migrate:status": "npx sequelize-cli db:migrate:status",
    "deploy:production": "node scripts/deploy-to-production.js",
    "seed:mailing-lists": "ts-node src/scripts/seed-mailing-lists.ts",
    "seed:practice-sessions": "ts-node src/scripts/seed-practice-sessions.ts",
    "seed:team-memberships": "ts-node src/scripts/seed-team-memberships.ts",
    "seed:production": "ts-node src/scripts/seed-production-from-dev.ts",
    "seed:practice-sessions-teams-2-3": "ts-node src/scripts/seed-practice-sessions-teams-2-3.ts",
    "seed:team-memberships-team-2": "ts-node src/scripts/seed-team-memberships-team-2.ts",
    "test:db-connection": "ts-node src/scripts/test-db-connection.ts",
    "test:models": "ts-node src/scripts/test-models.ts",
    "test:sheets-data": "ts-node src/scripts/test-google-sheets-data.ts",
    "check:config": "ts-node src/scripts/check-config.ts",
    "debug:connection": "ts-node src/scripts/debug-connection.tfs",
    "debug:sheets": "ts-node src/scripts/debug-google-sheets.ts",
    "clear:attendance": "ts-node src/scripts/clear-attendance.ts",
    "reset:attendance-sequence": "ts-node src/scripts/reset-attendance-sequence.ts",
    "fix:athlete-status": "ts-node src/scripts/fix-athlete-status.ts",
    "debug:athletes": "ts-node src/scripts/debug-athletes.ts",
    "seed:set-default-pins": "ts-node src/scripts/set-default-pins.ts",
    "seed:assign-usra-categories": "ts-node src/scripts/assign-usra-categories.ts",
    "seed:assign-usra-categories:prod": "node dist/scripts/assign-usra-categories.js"
  },
  "keywords": [],
  "author": "Edwin Escobar",
  "license": "UNLICENSED",
  "dependencies": {
    "dotenv": "^17.2.3",
    "googleapis": "^162.0.0",
    "pg": "^8.16.3",
    "sequelize": "^6.37.7",
    "sequelize-cli": "^6.6.3",
    "typescript": "^5.9.3",
    "winston": "^3.18.3"
  },
  "devDependencies": {
    "@types/node": "^24.7.2",
    "@types/pg": "^8.15.5",
    "@types/sequelize": "^4.28.20",
    "eslint": "^9.37.0",
    "jest": "^30.2.0",
    "prettier": "^3.6.2",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2"
  }
}
