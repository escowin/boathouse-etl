{
  "name": "boathouse-etl",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "dev": "ts-node src/index.ts",
    "start": "node dist/index.js",
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "validate": "npm run lint && npm run test && npm run build",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "format:check": "prettier --check src/**/*.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "setup": "npx sequelize-cli db:migrate --to 20250115000004-setup-complete-database.js",
    "setup:rollback": "npx sequelize-cli db:migrate:undo --to 20250115000004-setup-complete-database.js",
    "etl": "ts-node src/etl/index.ts",
    "etl:full": "ts-node src/etl/index.ts full",
    "etl:boats": "ts-node src/etl/index.ts boats",
    "etl:usra-categories": "ts-node src/etl/index.ts usra-categories",
    "etl:athletes": "ts-node src/etl/index.ts athletes",
    "etl:teams": "ts-node src/etl/index.ts teams",
    "etl:practice-sessions": "ts-node src/etl/index.ts practice-sessions",
    "etl:attendance": "ts-node src/etl/index.ts attendance",
    "etl:lineup": "ts-node src/etl/index.ts lineup",
    "etl:test": "ts-node src/etl/index.ts test",
    "etl:validate": "ts-node src/etl/index.ts validate",
    "etl:status": "ts-node src/etl/index.ts status",
    "migrate:up": "npx sequelize-cli db:migrate",
    "migrate:down": "npx sequelize-cli db:migrate:undo",
    "migrate:status": "npx sequelize-cli db:migrate:status",
    "deploy:production": "node scripts/deploy-to-production.js",
    "seed:mailing-lists": "ts-node src/scripts/seed-mailing-lists.ts",
    "test:db-connection": "ts-node src/scripts/test-db-connection.ts",
    "test:models": "ts-node src/scripts/test-models.ts",
    "test:sheets-data": "ts-node src/scripts/test-google-sheets-data.ts",
    "check:config": "ts-node src/scripts/check-config.ts",
    "debug:connection": "ts-node src/scripts/debug-connection.ts",
    "debug:sheets": "ts-node src/scripts/debug-google-sheets.ts",
    "clear:attendance": "ts-node src/scripts/clear-attendance.ts",
    "reset:attendance-sequence": "ts-node src/scripts/reset-attendance-sequence.ts",
    "auth:dev": "ts-node src/auth/server.ts",
    "auth:build": "tsc && node dist/auth/server.js",
    "auth:start": "node dist/auth/server.js",
    "auth:set-default-pins": "ts-node src/scripts/set-default-pins.ts",
    "debug:athletes": "ts-node src/scripts/debug-athletes.ts"
  },
  "keywords": [],
  "author": "Edwin Escobar",
  "license": "UNLICENSED",
  "dependencies": {
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^4.18.2",
    "express-rate-limit": "^8.0.1",
    "googleapis": "^162.0.0",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.16.3",
    "sequelize": "^6.37.7",
    "sequelize-cli": "^6.6.3",
    "typescript": "^5.9.3",
    "winston": "^3.18.3"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^24.7.2",
    "@types/pg": "^8.15.5",
    "@types/sequelize": "^4.28.20",
    "eslint": "^9.37.0",
    "jest": "^30.2.0",
    "prettier": "^3.6.2",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2"
  }
}
