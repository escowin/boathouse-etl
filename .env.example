# Database Configuration
DB_HOST=localhost
DB_PORT=5432
DB_NAME=boathouse_db
DB_USER=your_db_user
DB_PASSWORD=your_db_password
DB_SSL=false

# Google Sheets Configuration
GOOGLE_SHEETS_CREDENTIALS_PATH=./credentials/google-service-account.json
GOOGLE_SHEETS_SPREADSHEET_ID=your_spreadsheet_id_here
GOOGLE_SHEETS_ATHLETES_SHEET_NAME=Athletes
GOOGLE_SHEETS_BOATS_SHEET_NAME=Boats
GOOGLE_SHEETS_PRACTICE_SESSIONS_SHEET_NAME=Practice Sessions

# ETL Configuration
ETL_BATCH_SIZE=100
ETL_RETRY_ATTEMPTS=3
ETL_RETRY_DELAY_MS=5000
ETL_LOG_LEVEL=info

# Application Configuration
NODE_ENV=development
PORT=3000
LOG_FILE_PATH=./logs/etl.log

# Security
JWT_SECRET=your_jwt_secret_here
API_KEY=your_api_key_here

# External Services
WEBHOOK_URL=your_webhook_url_here
NOTIFICATION_EMAIL=admin@yourdomain.com